{
    "name": "beelab/symfony-beelab",
    "type": "project",
    "description": "Adapt this file with your actual project content",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        },
        {
            "name": "Mauro D'Alatri",
            "email": "mauro.dalatri@bee-lab.net"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "ramsey/uuid-doctrine": "^1.4",
        "simple-bus/symfony-bridge": "^5.1",
        "symfony/asset": "^3.4",
        "symfony/console": "^3.4",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-bundle": "^3.4",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/yaml": "^3.4",
        "twig/extensions": "^1.5"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*"
    },
    "require-dev": {
        "beelab/test-bundle": "^2.0",
        "dama/doctrine-test-bundle": "^5.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.9",
        "phpunit/phpunit": "^8.0",
        "symfony/browser-kit": "^3.4",
        "symfony/css-selector": "^3.4",
        "symfony/debug-bundle": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/phpunit-bridge": "^3.4",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "3.4.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Beelab\\": "src/Beelab/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fixtures\\": "fixtures/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    }
}
