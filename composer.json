{
    "name": "beelab/symfony-beelab",
    "type": "project",
    "description": "Adapt this file with your actual project content",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        },
        {
            "name": "Mauro D'Alatri",
            "email": "mauro.dalatri@bee-lab.net"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "symfony/flex": "^2.2"
    },
    "flex-require": {
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.12",
        "ramsey/uuid-doctrine": "^1.8",
        "symfony/asset": "^5.4",
        "symfony/console": "^5.4",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/messenger": "^5.4",
        "symfony/monolog-bundle": "^3.8",
        "symfony/security-bundle": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/web-link": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "replace": {
        "doctrine/annotations": "^1.13.1",
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php80": "*"
    },
    "flex-require-dev": {
        "beelab/test-bundle": "^5.4",
        "dama/doctrine-test-bundle": "^7.1",
        "deployer/deployer": "^7.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.5",
        "symfony/debug-bundle": "^5.4",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4"
        }
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application/",
            "Domain\\": "src/Domain/",
            "Infrastructure\\": "src/Infrastructure/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    }
}
